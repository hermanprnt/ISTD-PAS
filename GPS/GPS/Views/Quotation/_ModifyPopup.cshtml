<div id="modify-popup" class="modal fade">
    <div class="modal-dialog modal-lg">
        <div class="modal-content font-small">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title"><div id="Headpopup" style="float: left;">&nbsp;</div>&nbsp;- Pop Up</h4>
            </div>
            <div class="modal-body">
                <form role="form" class="form-horizontal">
                    <div class="row">
                        <div class="col-xs-12">
                            
                            <div class="form-group form-group-sm">
                                <label class="col-sm-4 control-label text-muted">Quotation Description</label>
                                <div class="col-sm-5">
                                    <textarea id="txtModifyQuotationDesc" class="form-control" style="height: 67px; width: 262px; max-height: 67px; max-width: 262px;"></textarea>
                                </div>
                            </div>
                            
                            <div class="form-group form-group-sm">
                                <label class="col-sm-4 control-label text-muted">Supplier</label>
                                <div class="col-sm-5">
                                    <input id="txtModifySupplier" type="text" class="form-control" />
                                </div>
                            </div>
                            
                            <div class="form-group form-group-sm">
                                <label class="col-sm-4 control-label text-muted">Quotation Date</label>
                                <div class="col-sm-5">
                                    <input id="txtModifyQuotationDate" type="text" class="form-control _datePicker" />
                                </div>
                            </div>

                            <div class="form-group form-group-sm">
                                <label class="col-sm-4 control-label text-muted">Value</label>
                                <div class="col-sm-5">
                                    <input id="txtModifyValue" type="text" class="form-control" style="text-align: right;" onkeypress="return event.charCode >= 48 && event.charCode <= 57" value="0" />
                                </div>
                            </div>

                            <div class="form-group form-group-sm">
                                <label class="col-sm-4 control-label text-muted">File</label>
                                <div class="col-sm-5">
                                    <input id="txtModifyFile" type="file" class="form-control" />
                                </div>
                                <button type="button" class="btn btn-primary btn-standard" style="padding: 3px 12px;">Upload</button>
                            </div>

                        </div>    
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <div class="col-xs-12 text-right">
                    <button type="button" class="btn btn-primary btn-standard">Save</button>
                    <button type="button" class="btn btn-danger btn-standard" data-dismiss="modal">Cancel</button>
                </div>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->