@using GPS.Models.Master

<div class="modal fade" data-keyboard="false" data-backdrop="static" id="PopUpTolerance"
    tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    
<div class="modal-dialog"  style="width: 1200px">
<form  method="post" action="javascript: onSubmitTolerance();" >
    <div class="modal-content">
        <div class="modal-header">           
            <div class="modal-title" id="myModalLabel">
                <h4 class="modal-title dettitle">Open Tolerance</h4>
            </div>
            <input type="hidden" id="txtTolHide"/>
        </div>     
        <div class="modal-body">
            <div class="row">
                <div class="form-horizontal">
                    <div class="col-xs-12">                         
                        <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">                            
                                <label class="col-sm-3 col-md-4 control-label text-muted"> 
                                    Periode</label>                                   
                                <div class="col-sm-8 col-md-8">  
                                      <input type="text" id="txtPeriodTol" class="form-control" readonly /> 
                                      <input type="hidden" id="txtPeriodTolHide"/>                                                 
                                </div>                                    
                            </div>
                        </div>                                                                                                       
                    </div>
                </div>
            </div> 
            <div class="row">
                <div class="form-horizontal">
                    <div class="col-xs-12">                         
                        <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">                            
                                <label class="col-sm-3 col-md-4 control-label text-muted"> 
                                    Division</label>                                   
                                <div class="col-sm-8 col-md-8">  
                                      <input type="text" id="txtDivTol" class="form-control" readonly /> 
                                      <input type="hidden" id="txtDivTolHide"/>                                                  
                                </div>                                    
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-12 col-md-4">
                              <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    Coordinator</label>
                                <div class="col-sm-5 col-md-8">
                                      <input type="text" id="txtCoordTol" class="form-control" readonly/>                                                                       
                                </div>
                            </div>                           
                        </div>  
                        <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    WBS No</label>
                                <div class="col-sm-5 col-md-8">
                                    <input type="text" id="txtWBSTol" class="form-control" readonly/>                                                                   
                                </div>
                            </div>
                        </div>                                                                                     
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-horizontal">
                    <div class="col-xs-12">                                              
                        <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    Order Type</label>
                                <div class="col-sm-5 col-md-8">
                                     <input type="text" id="txtTypeTol" class="form-control" readonly/>  
                                     <input type="hidden" id="txtTypeTolHide"/>                                                                   
                                </div>
                            </div>
                        </div>   
                        <div class="col-xs-12 col-sm-12 col-md-4">
                             <div class="form-group form-group-xs">                            
                                <label class="col-sm-3 col-md-4 control-label text-muted"> 
                                    Quota Amount</label>                                   
                                <div class="col-sm-8 col-md-8">  
                                    <input type="text" id="txtQuotaAmountTol" style="text-align:right" class="form-control" readonly/>                                                                                         
                                </div>                                    
                            </div>                            
                        </div> 
                        <div class="col-xs-12 col-sm-12 col-md-4">
                             <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    Additional Quota</label>
                                <div class="col-sm-5 col-md-8">
                                      <input type="text" id="txtAddTol" style="text-align:right" class="form-control" readonly/>                                                                                                           
                                </div>
                            </div>
                        </div>                                                                                                        
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="form-horizontal">
                     <div class="col-xs-12"> 
                         <div class="col-xs-12 col-sm-12 col-md-4">
                             <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    Usage</label>
                                <div class="col-sm-5 col-md-8">
                                      <input type="text" id="txtUsageTol" style="text-align:right" class="form-control" readonly/>                                                                                                           
                                </div>
                            </div>                          
                        </div> 
                         <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">                            
                                <label class="col-sm-3 col-md-4 control-label text-muted"> 
                                    Remaining Quota</label>                                   
                                <div class="col-sm-8 col-md-8">  
                                    <input type="text" id="txtRemainQuotaTol" style="text-align:right" class="form-control" readonly/>                                                                                      
                                </div>                                    
                            </div>  
                        </div> 
                         <div class="col-xs-12 col-sm-12 col-md-4">
                            <div class="form-group form-group-xs">
                                <label class="col-sm-4 col-md-4 control-label text-muted">
                                    Tolerance</label>
                                <div class="col-sm-5 col-md-8">
                                      <input type="text" id="txtTol" style="text-align:right" class="form-control"
                                        onkeypress="javascript:hapus(this,this.value);"
                                        onblur="if (this.value){this.onchange();} javascript:muncul(this,this.value);"
                                        onkeyup="javascript:detectComma(this.value,this);"/>                                                                     
                                </div>
                            </div>
                        </div>                          
                     </div>   
                </div>
            </div>           
            <div class="row">
                <div class="modal-footer">
                    <div class="col-xs-6">
                    </div>
                <div class="col-xs-6 text-right">
                    <div class="form-group-xs">
                        <button type="submit" class="btn btn-primary btn-xs">
                            Submit</button>
                         <button type="button" class="btn btn-danger btn-xs" onclick="CancelTolerance()">
                            Cancel</button>                           
                    </div>
                </div>
                </div>
            </div>
        </div>
    </div>
</form>
</div>

</div>