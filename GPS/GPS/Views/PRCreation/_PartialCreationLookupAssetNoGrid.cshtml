@using GPS.Models.PR.PRCreation
@{
    List<PRCreation> lpr = ViewData["AssetNo"] != "" ? (List<PRCreation>)ViewData["AssetNo"] : new List<PRCreation>();
}
<div class="table-responsive">
    <table id="LookUpTable-glclass" class="table table-striped table-bordered table-condensed gl-lookup-table" >
        <thead>
            
            <tr>
                <th class="text-center control-label text-muted" style="width:10%">Assset No</th>
                <th class="text-center control-label text-muted" style="width:5%">Sub Asset</th>
                <th class="text-center control-label text-muted" style="width:5%">Category</th>
                <th class="text-center control-label text-muted" style="width:10%">Asset Class</th>
                <th class="text-center control-label text-muted" style="width:20%">Location</th>
                <th class="text-center control-label text-muted" style="width:20%">Description</th>
                <th class="text-center control-label text-muted" style="width:10%">Created By</th>
                <th class="text-center control-label text-muted" style="width:10%">Created Date</th>
            </tr>
        </thead>
        <tbody id="bodylookup-glclass">
            @if (lpr.Count > 0)
            {
                foreach (PRCreation pr in lpr)
                { 
                    <tr ondblclick="javascript:MoveVendorKeyAssetNo('@pr.ASSET_CATEGORY', '@pr.CLASS_ID', '@pr.LOCATION_ID', '@pr.ASSET_NO', '@pr.SUB_ASSET_NO')" style="cursor:pointer">
                        <td>@pr.ASSET_NO</td>
					    <td>@pr.SUB_ASSET_NO</td>
                        <td>@pr.ASSET_CATEGORY</td>
                        <td>@pr.CLASS_ID</td>
                        <td>@pr.LOCATION_ID</td>
                        <td>@pr.ASSET_DESC</td>
                        <td>@pr.CREATED_BY</td>
                        <td>@pr.CREATED_DATE</td>
                        

                    </tr>
                }
            }
            else
            {
                <tr>
                    <td colspan="8" class="text-center">No Asset Data</td>
                </tr> 
            }
        </tbody>
        <tfoot>
            <tr>
                <td colspan="8">
                    @Html.Partial("_UIPaging")
                </td>
            </tr>
        </tfoot>
    </table>
</div>