@using GPS.Models.PRPOApproval
@{
    CommonApprovalResult prApprovalResult = (CommonApprovalResult)ViewData["PRApprovalResult"];
}

<div class="modal-dialog modal-md" style="width:20%;max-height:45%">
    <div class="modal-content">
        <div class="modal-header">
            <b>
                <h4 class="modal-title">@(prApprovalResult.ACTION) Information</h4>
            </b>
        </div>
        <div class="modal-body">
            <div class="form-group">
                <label>Document Processed : @(prApprovalResult.DOC_COUNT)</label><br />
                <label>Item Processed : @(prApprovalResult.ITEM_COUNT)</label><br />
                <label>Item Success : @(prApprovalResult.SUCCESS)</label><br />
                <label>Item Fail : @(prApprovalResult.FAIL)</label><br />
                @{if(prApprovalResult.FAIL > 0){
                    <label>@prApprovalResult.MESSAGE.ToHtmlString()</label><br />
                 }}
            </div>
        </div>
        <div class="modal-footer">
            <div class="text-center">
                <button id="btn-close-dialog" type="button" class="btn btn-primary" data-dismiss="modal">Yes</button>
            </div>
        </div>
    </div>
</div>