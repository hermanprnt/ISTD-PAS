<div class="modal fade" id="ExchangeRateModal" tabindex="-1" role="dialog" aria-labelledby="basicModal" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="myModalLabel">Exchange Rate Data</h4>
                <input type="hidden" id="txtId" />
            </div>
            <form id="formVendor" method="post" action="javascript: onSaveChange();">
                <div class="modal-body">
                <div class="alert alert-danger" role="alert" style="display:none" id="alert-searchModal"></div>
                <div class="alert alert-success" role="alert" style="display:none" id="success-searchModal"></div>
                <div class="alert alert-warning" role="alert" style="display:none" id="warning-searchModal"></div>
                <div class="alert alert-info" role="alert" style="display:none" id="info-searchModal"></div>
                    <div class="form-horizontal">
                        <div class="form-group">
                            <label class="col-sm-4 control-label text-muted required-left">Currency Code</label>
                            <div class="col-sm-8">
                                <input type="text" id="txtMCurrCode" class="form-control" required />
                            </div>
                        </div>
                        <div class="form-group form-group-sm">
                            <label class="col-sm-4 control-label text-muted required-left">Exchange Rate (Rp.)</label>
                            <div class="col-sm-8">
                                <input type="text" id="txtMExchangeRate" class="form-control" required />
                            </div>
                        </div>
                        <div class="form-group form-group-sm">
                            <label class="col-sm-4 control-label text-muted required-left">Valid From</label>
                            <div class="col-sm-8">
                                <input id="dtMValidDateFrom" type="text" class="form-control" data-date-format="dd.mm.yyyy" required />
                            </div>
                        </div>
                        <div class="form-group form-group-sm">
                            <label class="col-sm-4 control-label text-muted">Valid To</label>
                            <div class="col-sm-8">
                                <input id="dtMValidDateTo" type="text" disabled="disabled" class="form-control _datepicker" value="31.12.9999" />
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary btn-xs" id="btnSave">Save</button>
                    <button type="button" class="btn btn-danger btn-xs" id="btnCancel" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
